{
  "name": "bloom-frontend",
  "version": "0.1.0",
  "private": true,
  "resolutions": {
    "cross-spawn": "^7.0.5",
    "brace-expansion": "^2.0.2",
    "systeminformation": "^5.27.14"
  },
  "engines": {
    "node": ">=22.x"
  },
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "NODE_OPTIONS='-r newrelic' next start",
    "https:proxy": "local-ssl-proxy --source 3010 --target 3000 --cert localhost.pem --key localhost-key.pem",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "type-check": "tsc -p tsconfig.json",
    "format": "prettier . --write",
    "format:precommit": "prettier --write .",
    "test": "jest",
    "test:watch": "jest --watch",
    "cypress": "cypress open",
    "cypress:headless": "cypress run --headless true --browser chrome",
    "cypress:headless-spec": "cypress run --headless true --browser chrome --spec $SPEC",
    "cypress:coverage": "npx nyc report --reporter=text-summary",
    "cypress:auth": "cypress run --spec \"cypress/integration/tests/auth-redirect.cy.tsx,cypress/integration/tests/create-user*.cy.tsx,cypress/integration/tests/reset-password.cy.tsx\"",
    "cypress:user-accounts": "cypress run --spec \"cypress/integration/tests/delete-user.cy.tsx,cypress/integration/tests/user-account-settings.cy.tsx,cypress/integration/tests/user-disable-service-emails.cy.tsx,cypress/integration/tests/user-about-you.cy.tsx,cypress/integration/tests/user-update-email.cy.tsx,cypress/integration/tests/user-apply-a-code.cy.tsx\"",
    "cypress:admin": "cypress run --spec \"cypress/integration/tests/admin-dashboard.cy.tsx,cypress/integration/tests/partner-admin.cy.tsx\"",
    "cypress:user-content": "cypress run --spec \"cypress/integration/tests/activities.cy.tsx,cypress/integration/tests/notes.cy.tsx,cypress/integration/tests/user-course-session-behaviour.cy.tsx,cypress/integration/tests/therapy-usage.cy.tsx,cypress/integration/tests/user-messaging.cy.tsx\"",
    "cypress:partners": "cypress run --spec \"cypress/integration/tests/create-access-code.badoo.cy.tsx,cypress/integration/tests/register-partner*.cy.tsx,cypress/integration/tests/welcome*.cy.tsx\"",
    "cypress:system": "cypress run --spec \"cypress/integration/tests/action-handler.cy.tsx,cypress/integration/tests/cookie-consent.cy.tsx\"",
    "cypress:public-content": "cypress run --spec \"cypress/integration/tests/home.cy.tsx,cypress/integration/tests/meet-the-team.cy.tsx,cypress/integration/tests/grounding-exercises.cy.tsx,cypress/integration/tests/initial-exploration.cy.tsx,cypress/integration/tests/maintenance.cy.tsx,cypress/integration/tests/navigation.cy.tsx,cypress/integration/tests/not-found.cy.tsx,cypress/integration/tests/sitemap.cy.tsx\"",
    "check:translations": "node ./scripts/checkTranslation.js",
    "check:links": "node ./scripts/checkLinks.mjs",
    "postinstall": "husky",
    "prepare": "husky",
    "knip": "knip"
  },
  "dependencies": {
    "@emotion/cache": "^11.14.0",
    "@emotion/react": "^11.14.0",
    "@emotion/server": "^11.11.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^7.3.1",
    "@mui/lab": "^7.0.1-beta.20",
    "@mui/material": "^7.3.1",
    "@mui/material-nextjs": "^7.1.0",
    "@next/third-parties": "16.1.0",
    "@reduxjs/toolkit": "^2.11.2",
    "@rollbar/react": "^1.0.0",
    "@storyblok/react": "^5.4.20",
    "@types/dompurify": "^3.0.5",
    "@vercel/analytics": "^1.5.0",
    "axios": "^1.9.0",
    "brace-expansion": "^4.0.1",
    "cross-spawn": "^7.0.0",
    "date-fns": "^4.1.0",
    "dompurify": "^3.3.1",
    "firebase": "^12.7.0",
    "gemoji": "^8.1.0",
    "js-cookie": "^3.0.1",
    "minimatch": "^10.0.3",
    "newrelic": "^13.8.1",
    "next": "16.1.0",
    "next-intl": "^4.6.0",
    "next-pwa": "^5.6.0",
    "nextjs-hotjar": "^1.2.0",
    "nuka-carousel": "^8.2.0",
    "phone": "^3.1.68",
    "react": "19.2.3",
    "react-cookie-consent": "^9.0.0",
    "react-dom": "19.2.3",
    "react-international-phone": "^4.0.4",
    "react-player": "^3.4.0",
    "react-redux": "^9.2.0",
    "rollbar": "^2.26.4",
    "storyblok-rich-text-react-renderer": "^3.0.1"
  },
  "devDependencies": {
    "@cypress/code-coverage": "^3.14.0",
    "@eslint/eslintrc": "^3.3.3",
    "@next/bundle-analyzer": "16.1.1",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/react": "^16.3.1",
    "@types/jest": "^30.0.0",
    "@types/js-cookie": "^3.0.2",
    "@types/newrelic": "^9.14.8",
    "@types/node": "^25.0.6",
    "@types/react": "19.2.8",
    "babel-plugin-istanbul": "^7.0.0",
    "cypress": "^15.8.2",
    "dotenv": "^17.2.3",
    "eslint": "9.39.2",
    "eslint-config-next": "16.1.1",
    "eslint-config-prettier": "^10.1.5",
    "husky": "^9.0.11",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^30.0.3",
    "jest-environment-jsdom": "^30.2.0",
    "knip": "^5.80.2",
    "mailslurp-client": "^16.0.0",
    "nyc": "^17.0.0",
    "pre-commit": "^1.2.2",
    "prettier": "^3.5.3",
    "typescript": "^5.8.3"
  }
}
